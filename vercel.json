{
  "routes": [
    {
      "src": "/miaogu/pc/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/",
      "dest": "/miaogu/pc/"
    },
    {
      "src": "/miaogu/mobile",
      "dest": "https://app.miaogu.top"
    },
    {
      "src": "/miaogu/api/(.*)",
      "dest": "https://yuki.cmyam.net/miaogu/api/$1"
    }
  ]
}
