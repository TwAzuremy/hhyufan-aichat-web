{
  "routes": [
    {
      "src": "/miaogu/pc/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/miaogu/pc/"
    },
    {
      "source": "/miaogu/mobile",
      "destination": "https://app.miaogu.top"
    },
    {
      "source": "/miaogu/api/(.*)",
      "destination": "https://yuki.cmyam.net/miaogu/api/$1"
    }
  ]
}